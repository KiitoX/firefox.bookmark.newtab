{
  "manifest_version": 2,
  "name": "bookmark.newtab",
  "version": "1.0.5",
  "developer": {
    "name": "Manuel"
  },
  "description": "Shows a page with a bookmark bar instead of the default tab page",
  "applications": {
    "gecko": {
      "id": "@bookmarknewtab"
    }
  },
  "page_action": {
    "default_title": "Store favicon",
    "default_icon": {
      "19": "ico-19.png",
      "38": "ico-38.png"
    },
    "show_matches": [
      "http://*/*",
      "https://*/*"
    ],
    "pinned": false
  },
  "background": {
    "scripts": [
      "clearClosedTabs.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "sessions",
    "tabs",
    "storage",
    "bookmarks",
    "history"
  ]
}
